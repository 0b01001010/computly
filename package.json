		"@typescript-eslint/parser": "^5.13.0",
		"dayjs": "^1.10.8",
		"dotenv": "^16.0.0",
		"eslint": "^7.32.0",
		"eslint-config-prettier": "^8.5.0",
		"eslint-plugin-svelte3": "^3.4.1",
		"front-matter": "^4.0.2",
		"husky": "^7.0.4",
