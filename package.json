{
	"name": "computly",
	"homepage": "https://computly.me",
	"version": "1.0.0",
	"license": "MIT",
	"scripts": {
		"dev": "svelte-kit dev",
		"build": "pnpm run generate:images && pnpm run generate:sitemap && svelte-kit build",
		"preview": "svelte-kit preview",
		"check": "svelte-check --tsconfig ./tsconfig.json --compiler-warnings 'css-unused-selector:ignore' --threshold 'error'",
		"check:watch": "svelte-check --tsconfig ./tsconfig.json --watch",
		"lint": "prettier --ignore-path .gitignore --ignore-path .prettierignore --write --plugin-search-dir=. . && eslint --ignore-path .gitignore .",
		"generate:images": "node --loader ts-node/esm ./generate-images.ts",
		"generate:sitemap": "node ./generate-sitemap.js",
		"prepare": "husky install"
	},
	"devDependencies": {
		"@sveltejs/adapter-vercel": "^1.0.0-next.47",
		"@sveltejs/kit": "next",
		"@types/node": "^17.0.23",
		"@types/sharp": "^0.30.0",
		"@typescript-eslint/eslint-plugin": "^5.17.0",
		"@typescript-eslint/parser": "^5.17.0",
		"dotenv": "^16.0.0",
		"eslint": "^8.12.0",
		"eslint-config-prettier": "^8.5.0",
		"eslint-plugin-svelte3": "^3.4.1",
		"front-matter": "^4.0.2",
		"husky": "^7.0.4",
		"kleur": "^4.1.4",
		"mdsvex": "^0.10.5",
		"prettier": "^2.6.1",
		"prettier-plugin-svelte": "^2.6.0",
		"sass": "^1.49.9",
		"svelte": "^3.46.4",
		"svelte-check": "^2.4.6",
		"svelte-meta-tags": "^2.5.1",
		"svelte-preprocess": "^4.10.4",
		"ts-node": "^10.7.0",
		"tslib": "^2.3.1",
		"typescript": "^4.6.3",
		"vite-imagetools": "^4.0.3"
	},
	"optionalDependencies": {
		"sharp": "^0.30.3"
	},
	"type": "module",
	"dependencies": {
		"papercss": "^1.8.3",
		"postcss": "^8.4.12",
		"postcss-html": "^1.3.0",
		"spaper": "^0.5.6",
		"svelte-lazy-loader": "^1.0.0"
	}
}
